{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "postcss-import": "^13.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-gatsby": "^0.7.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-styled-components": "^3.5.0",
    "gatsby-plugin-typescript": "^2.7.0",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.2",
    "tailwind.macro": "^0.5.10",
    "tailwindcss": "^2.0.1",
    "twin.macro": "^2.0.4",
    "typescript": "^4.1.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "build:css": "postcss src/styles/globals.css -o src/styles/styles.css",
    "watch:css": "postcss src/styles/globals.css -o src/assets/styles.css",
    "dev": "npm run watch:css && npm run develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
